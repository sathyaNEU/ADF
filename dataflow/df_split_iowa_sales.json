				"          {Store Name} = Store_Name,",
				"          Address,",
				"          City,",
				"          {Zip Code} = Zip_Code,",
				"          {Store Location} = Store_Location,",
				"          {County Number} = County_Number,",
				"          County,",
				"          Category,",
